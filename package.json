{
  "name": "stolen-bike-cases-nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "app:start": "node ./dist/",
    "start": "rimraf dist && copyfiles -u 2 src/email-templates/*.hbs dist/email-templates/ && tsc-watch --outDir ./dist --onSuccess \"npm run app:start\" --onFailure \"'echo Compilation Failed'\"",
    "build": "rimraf dist && tsc && copyfiles -u 2 src/email-templates/*.hbs dist/email-templates/",
    "tsc": "rimraf dist && npm run lint && tsc --watch",
    "migrate:up": "npm run build && npx sequelize-cli-typescript db:migrate",
    "migrate:down": "npm run build && npx sequelize-cli-typescript db:migrate:undo",
    "create:db": "node_modules/.bin/sequelize db:create",
    "first": "sequelize-cli db:create && npx sequelize-cli-typescript db:migrate"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "mysql2": "^2.2.5",
    "sequelize": "^6.3.5",
    "sequelize-typescript": "^1.1.0",
    "sequelize-cli": "^6.2.0",
    "sequelize-cli-typescript": "^3.2.0-c",
    "helmet": "^4.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "joi": "^17.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/express-rate-limit": "^5.1.0",
    "@types/sequelize": "^4.28.9",
    "@types/morgan": "^1.9.1",
    "@types/helmet": "^0.0.48",
    "@types/joi": "^14.3.4",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "copyfiles": "^2.4.0",
    "husky": "^4.3.0",
    "rimraf": "^3.0.2",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "eslint": "^7.10.0",
    "ts-node": "^9.0.0",
    "ts-watch": "^1.0.8",
    "morgan": "^1.10.0",
    "typescript": "^4.0.3"
  },
  "author": "",
  "license": "ISC"
}
